<div id="new-cash-costsheet" v-cloak>
  <div class="uk-container uk-margin-auto" uk-grid>
    <div class="uk-width-3-5 uk-margin-auto uk-padding-remove">
      <ul class="uk-breadcrumb">
        <li><a href="">Create New</a></li>
        <li><a href="">Buyer's Cost Sheet</a></li>
        <li><span>Cash Buyer</span></li>
      </ul>
      <ajax-form class="uk-form-stacked" action="newCostsheet" :handle-parsing="handleParsingNewCostsheetForm" @submitted="submittedNewCostsheetForm">
        <fieldset class="uk-fieldset">
          <legend class="uk-legend uk-text-primary uk-text-uppercase">Demographics</legend>
          <div class="uk-margin">
            <label class="uk-form-label" for="clientName">Client's Name</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="text" id="clientName" name="client" v-model="newFormData.client" placeholder="Foster Breckenridge">
            </div>
          </div>
          <div class="uk-margin">
            <label class="uk-form-label" for="address">Property Address</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="text" id="address" name="address" v-model="newFormData.address" placeholder="123 Oak St">
            </div>
          </div>
          <div class="uk-margin">
            <label class="uk-form-label" for="county">County</label>
            <div class="uk-form-controls">
              <select class="uk-select" id="county" name="county"  v-model.number="newFormData.county">
                <option value="0" disabled="disabled">Select One</option>
                <option value="1">Douglas or Sarpy</option>
                <option value="2" disabled="disabled">All Others</option>
              </select>
            </div>
          </div>
          <div class="uk-margin">
            <label class="uk-form-label" for="offerStatus">Offer Status</label>
            <div class="uk-form-controls">
              <select class="uk-select" id="offerStatus" name="offerStatus" v-model.number="newFormData.offerStatus">
                <option value="0" disabled="disabled">Select One</option>
                <option value="1">Initial</option>
                <option value="2">Counter</option>
                <option value="3">Accepted</option>
              </select>
            </div>
          </div>
          <div class="uk-margin">
            <label class="uk-form-label" for="closingDate">Closing Date</label>
            <div class="uk-form-controls">
              <vuejs-datepicker :input-class="'uk-input'" :format="customFormatter" name="closingDate" placeholder="DD/MM/YYYY"></vuejs-datepicker>
            </div>
          </div>
        </fieldset>
        <fieldset class="uk-fieldset">
          <legend class="uk-legend uk-text-primary uk-text-uppercase">Financials</legend>
          <div class="uk-margin">
            <label class="uk-form-label" for="purchasePrice">Purchase Price</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="number" step="0.01" id="purchasePrice" name="purchasePrice" v-model.number="newFormData.purchasePrice" placeholder="250000.00" >
            </div>
          </div>
          <div class="uk-margin">
            <label class="uk-form-label" for="earnestDeposit">Earnest Deposit</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="number" step="0.01" id="earnestDeposit" name="earnestDeposit" v-model.number="newFormData.earnestDeposit" placeholder="1000.00" >
            </div>
          </div>
        </fieldset>
        <fieldset class="uk-fieldset">
          <legend class="uk-legend uk-text-primary uk-text-uppercase">Taxes and Insurance</legend>
          <div class="uk-margin">
            <label class="uk-form-label" for="propertyTax">Annual Property Taxes</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="number" step="0.01" id="propertyTax" name="propertyTax" v-model.number="newFormData.propertyTax" placeholder="3500.00" >
            </div>
          </div>
        </fieldset>
        <fieldset class="uk-fieldset">
          <legend class="uk-legend uk-text-primary uk-text-uppercase">Other Costs &amp; Fees</legend>
          <div class="uk-margin">
            <label class="uk-form-label" for="sellerPaid">Seller Paid Costs</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="number" step="0.01" id="sellerPaid" name="sellerPaid" v-model.number="newFormData.sellerPaid" placeholder="1500.00">
            </div>
          </div>
          <div class="uk-margin">
            <label class="uk-form-label" for="homeInspection">Home Inspection Fee</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="number" step="0.01" id="homeInspection" name="homeInspection" v-model.number="newFormData.homeInspection" placeholder="400.00">
            </div>
            <p class="uk-form-label uk-text-muted uk-margin-remove-top">Include only if buyer-paid at closing</p>
          </div>
          <div class="uk-margin">
            <label class="uk-form-label" for="radonInspection">Radon Inspection Fee</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="number" step="0.01" id="radonInspection" name="radonInspection" v-model.number="newFormData.radonInspection" placeholder="200.00">
            </div>
            <p class="uk-form-label uk-text-muted uk-margin-remove-top">Include only if buyer-paid at closing</p>
          </div>
          <div class="uk-margin">
            <label class="uk-form-label" for="homeWarranty">Home Warranty</label>
            <div class="uk-form-controls">
              <input class="uk-input" type="number" step="0.01" id="homeWarranty" name="homeWarranty" v-model.number="newFormData.homeWarranty" placeholder="595.00">
            </div>
            <p class="uk-form-label uk-text-muted uk-margin-remove-top">Include only if buyer-paid at closing</p>
          </div>
        </fieldset>
        <fieldset class="uk-fieldset">
          <legend class="uk-legend uk-text-primary uk-text-uppercase">View/Overwrite Standard Values</legend>
          <ul uk-accordion="multiple: true">
            <li>
                <a class="uk-accordion-title" href="#">Fees</a>
                <div class="uk-accordion-content">
                  <div class="uk-margin">
                    <label class="uk-form-label" for="brokerAdmin">Broker Administration Fee</label>
                    <div class="uk-form-controls">
                      <input class="uk-input" type="number" step="0.01" id="brokerAdmin" name="brokerAdmin" v-model.number="newFormData.brokerAdmin">
                    </div>
                  </div>
                  <div class="uk-margin">
                    <label class="uk-form-label" for="appraisalFee">Appraisal Fee</label>
                    <div class="uk-form-controls">
                      <input class="uk-input" type="number" step="0.01" id="appraisalFee" name="appraisalFee" v-model.number="newFormData.appraisalFee">
                    </div>
                  </div>
                  <div class="uk-margin">
                    <label class="uk-form-label" for="settlementFee">Title/Escrow Settlement Fee</label>
                    <div class="uk-form-controls">
                      <input class="uk-input" type="number" step="0.01" id="settlementFee" name="settlementFee" v-model.number="newFormData.settlementFee">
                    </div>
                  </div>
                  <div class="uk-margin">
                    <label class="uk-form-label" for="endorsements">ALTA Endorsement Fees</label>
                    <div class="uk-form-controls">
                      <input class="uk-input" type="number" step="0.01" id="endorsements" name="endorsements" v-model.number="newFormData.endorsements">
                    </div>
                  </div>
                  <div class="uk-margin">
                    <label class="uk-form-label" for="recordingFee">County Recording Fee</label>
                    <div class="uk-form-controls">
                      <input class="uk-input" type="number" step="0.01" id="recordingFee" name="recordingFee" v-model.number="newFormData.recordingFee">
                    </div>
                  </div>
                  <div class="uk-margin">
                    <label class="uk-form-label" for="plotPlan">Survey/Plot Plan</label>
                    <div class="uk-form-controls">
                      <input class="uk-input" type="number" step="0.01" id="plotPlan" name="plotPlan" vv-model.number="newFormData.plotPlan">
                    </div>
                  </div>
                  <div class="uk-margin">
                    <label class="uk-form-label" for="pestInspection">Pest Inspection</label>
                    <div class="uk-form-controls">
                      <input class="uk-input" type="number" step="0.01" id="pestInspection" name="pestInspection" v-model.number="newFormData.pestInspection">
                    </div>
                  </div>
                </div>
              </li>
        </fieldset>
        <ajax-button type="submit" class="uk-button uk-button-primary uk-width-1-1">Generate</ajax-button>
      </ajax-form>
    </div>
  </div>
</div>
<%- /* Expose server-rendered data as window.SAILS_LOCALS :: */ exposeLocalsToBrowser() %>
